            {"name": "delete", "assistant": "deleteAssistant", "public": true},
            {"name": "fileinfo", "assistant": "fileinfoAssistant", "public": true},
            {"name": "device", "assistant": "deviceAssistant", "public": true},
            {"name": "upgrade", "assistant": "upgradeAssistant", "public": true, "subscribe": true}
