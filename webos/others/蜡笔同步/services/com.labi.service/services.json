    "description": "file read write serivce",
    "engine": "node",
    "commandTimeout": 180,
    "activityTimeout": 180,
    "services": [
        {"name": "com.labi.service", "description": "read file", "commands": [
            {"name": "read", "assistant": "readAssistant", "public": true},
            {"name": "write", "assistant": "writeAssistant", "public": true},
